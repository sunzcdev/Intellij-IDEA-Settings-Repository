<templateSet group="Java">
  <template name="prm" value="private $RETURN_TYPE$ $METHOD_NAME$(){&#10;    &#10;}" description="创建私有方法" toReformat="false" toShortenFQNames="true">
    <variable name="RETURN_TYPE" expression="" defaultValue="void" alwaysStopAt="true" />
    <variable name="METHOD_NAME" expression="complete()" defaultValue="method" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pum" value="public $RETURN_TYPE$ $METHOD_NAME$(){&#10;    &#10;}" description="自动创建公有方法" toReformat="false" toShortenFQNames="true">
    <variable name="RETURN_TYPE" expression="" defaultValue="void" alwaysStopAt="true" />
    <variable name="METHOD_NAME" expression="" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>